(define/in-namespace std::math::fns
    (define (abs x)
        (if (< x 0)
            (- x)
            x)))

(define/in-namespace std::math::fns
    (define (square x)
        (* x x)))

(define/in-namespace std::math::fns
    (define (fact n)
        (if (<= n 1)
            1
            (* n (fact (- n 1))))))

(require std::math::fns)
